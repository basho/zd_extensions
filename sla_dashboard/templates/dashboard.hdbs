<h1>SLA Overview Dashboard</h1>
<br />
<table class="data_table" width="100%">
<tr class="priority_group urgent">
<td class="group_heading"><div class="header_text">urgent</div></td>
<td>        
        {{#each urgents}}
            <a           
title="Subject: {{subject}}
Status: {{status}}
SLA Basho Comment: {{sla_basho_comment}}
SLA Customer Comment: {{sla_customer_comment}}
last_update:{{last_update}}"

            href="#/tickets/{{id}}" id="ticket-{{id}}"  class="ticket">
                <svg id="dwg-{{id}}" class="ticket_clock"></svg>
                <div class="ticket_info">
                    <div class="ticket_number">{{id}}</div>
                    <div class="time_remaining">{{time_remaining}}</div>
                    <div class="assignee">{{assignee}}</div>
                </div>
            </a>
        {{/each}}
</td>
</tr>
